			"default": 0.01,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "SNP variance prior",
			"attribute": "-vara"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:decimal"
			]
		}
	}, {
		"id": "vare",
		"value": {
			"default": 0.01,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Error variance prior",
			"attribute": "-vare"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:decimal"
			]
		}
	}, {
		"id": "dfvara",
		"value": {
			"default": -2.0,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Degrees of freedom Va",
			"attribute": "-dfvara"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:deciaml"
			]
		}
	}, {
		"id": "dfvare",
		"value": {
			"default": -2.0,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Degrees of freedom Ve",
			"attribute": "-dfvare"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:decimal"
			]
		}
	}, {
		"id": "delta",
		"value": {
			"default": 1,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Prior for Dirichlet",
			"desciption" : "The pseudo-observation for each class of the component mixture model.",
			"attribute": "-delta"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {
		"id": "msize",
		"value": {
			"default": 0,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Number of SNPs in reduced update",
			"desciption" : "The number of SNPs with non-zero effects to sample within a cycle.",
			"attribute": "-msize"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {
		"id": "mrep",
		"value": {
			"default": 5000,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Reduced Update Full Cycle",
			"desciption" : "The number of full cycles to complete before reducing to the msize paramter.",
			"attribute": "-mrep"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {
		"id": "thin",
		"value": {
			"default": 10,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Thinning rate",
			"desciption" : "Which sample to use for posterior inference. 10 will use every 10th sample.",
			"attribute": "-thin"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {
		"id": "ndist",
		"value": {
			"default": 4,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Mixture distributions",
			"desciption" : "The number of mixture distributions. Their variances' must be defined by the gpin paramter",
			"attribute": "-ndist"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {
		"id": "gpin",
		"value": {
			"default": [0.0, 0.0001, 0.001, 0.01],
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Effect sizes of mixtures",
			"desciption" : "Defines the SNP variance of each component. The number of components is defined by the ndist paramter",
			"attribute": "-gpin"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			],
			"maxCardinality": -1
		}
	}, {
		"id": "predict",
		"value": {
			"default": false,
			"type": "bool",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Prediction flag",
			"desciption" : "Perform prediction analysis",
			"attribute": "-predict"
		},
		"semantics": {
			"ontology": [
				"xs:boolean"
			]
		}
	}, {
		"id": "snpout",
		"value": {
			"default": false,
			"type": "bool",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Output detailed SNP info flag",
			"desciption" : "Output each MCMC iteration to a file",
			"attribute": "-snpout"
		},
		"semantics": {
			"ontology": [
				"xs:boolean"
			]
		}
	}, {

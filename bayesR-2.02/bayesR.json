		"value": {
			"default": 4,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Number of SNPs in block",
			"attribute": "-blocksize"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {
		"id": "nthreads",
		"value": {
			"default": 4,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Number of threads",
			"attribute": "-nthreads"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {
		"id": "shuffle",
		"value": {
			"default": false,
			"type": "bool",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Permute order of blocks",
			"attribute": "-shuffle"
		},
		"semantics": {
			"ontology": [
				"xs:boolean"
			]
		}
	}]

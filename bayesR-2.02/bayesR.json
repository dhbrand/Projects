		"value": {
			"default": false,
			"type": "bool",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Permute flag",
			"desciption" : "Updates SNP effects in random order",
			"attribute": "-permute"
		},
		"semantics": {
			"ontology": [
				"xs:boolean"
			]
		}
	}, {
		"id": "numberit",
		"value": {
			"default": 50000,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Chain Length",
			"desciption" : "Length of MCMC chain",
			"attribute": "-numit"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {
		"id": "burnin",
		"value": {
			"default": 20000,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Burnin steps",
			"desciption" : "The amount of initial samples to discard",
			"attribute": "-burnin"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {
		"id": "seed",
		"value": {
			"default": 0,
			"type": "number",
			"visible": true,
			"required": false
		},
		"details":{
			"label": "Random Seed",
			"desciption" : "Initial value for random number",
			"attribute": "-seed"
		},
		"semantics": {
			"maxCardinality":1,
			"ontology": [
				"xs:int"
			]
		}
	}, {

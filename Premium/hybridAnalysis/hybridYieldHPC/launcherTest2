# this file is a list of all the R scripts that I want to run
# I directed the LAUNCHER_JOB_FILE to this for using the launcher module

var1=5
var2=6
var3=7
var4=8
var5=9
var6=10


Rscript ./launcherFunc.R  $var1 --no-save --no-restore --verbose launcherFunc.R > output$var1.Rout 2> error$var1.Rout
Rscript ./launcherFunc.R  $var2 --no-save --no-restore --verbose launcherFunc.R > output$var2.Rout 2> error$var2.Rout
Rscript ./launcherFunc.R  $var3 --no-save --no-restore --verbose launcherFunc.R > output$var3.Rout 2> error$var3.Rout
Rscript ./launcherFunc.R  $var4 --no-save --no-restore --verbose launcherFunc.R > output$var4.Rout 2> error$var4.Rout
Rscript ./launcherFunc.R  $var5 --no-save --no-restore --verbose launcherFunc.R > output$var5.Rout 2> error$var5.Rout
Rscript ./launcherFunc.R  $var6 --no-save --no-restore --verbose launcherFunc.R > output$var6.Rout 2> error$var6.Rout
